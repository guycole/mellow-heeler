--
-- Title: geoloc.psql
-- Description: time and location
-- 
create table geoloc (
  id               bigserial   primary key,
  accuracy         varchar(16) not null,
  altitude         varchar(16) not null,
  fix_time_ms      bigint      not null,
  latitude         varchar(16) not null,
  longitude        varchar(16) not null
);
