--
-- Title: wap.psql
-- Description: all the wap
-- 
create table wap (
  id          bigserial    primary key,
  bssid       varchar(24)  unique not null,
  capability  varchar(80)  not null,
  frequency   smallint     not null,
  ssid        varchar(80)  not null
);

create index wap_ssid on wap(ssid);
